 <%= form_for @user, html: {multipart:true} do |f| %>

 <%= render "shared/error_messages", object: f.object %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :address, "Enter your address (it will NOT be displayed publicly)" %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state_province, "State/province" %>
  <%= f.text_field :state_province %>

  <%= f.label :country %>
  <%= f.text_field :country %>

   <%= f.label :password %>
  <%= f.password_field :password %>

   <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.label :learning %>
  <%= f.select :learning, options_for_select([ "Javascript", "Ruby on Rails", "Python", "jQuery", "C++", "HTML & CSS", "PHP", "Biology", "Calculus", "Linear Algebra", "Differential Equations", "Physics", "Chemistry", "Medicine", "Statistics", "Electronics", "Android Development", "iOS Development", "Google Glass Development", "Tesla Development", "SmartThings Development", "Occulus Rift Development", "Emotiv Development", "Meta Developers", "Raspberry Pi Development"]) %>
  <p><em>Upload a profile picture</em></p>
 <%= f.file_field :image %>

 <div class="text-center"><%= f.submit class: "button radius" %></div>

<h1>Meeting Information</h1>

 <%= f.label :currently_available %>  
  <%= f.select :currently_available, options_for_select([ "Yes", "No", "Maybe"]) %>

 <%= f.label :preferred_gender_of_study_partner %>     
<%= f.select :preferred_gender, options_for_select([ "Male", "Female", "Any"]) %>

 <%= f.label :study_location_available %> 
 <%= f.select :study_location_available, options_for_select([ "Yes", "No"]) %>

 <%= f.label :smoking_allowed %>      
<%= f.select :smoking_allowed, options_for_select([ "Yes", "No"]) %>

 <%= f.label :can_host_children %>    
<%= f.select :can_host_children, options_for_select([ "Yes", "No"]) %>

 <%= f.label :can_host_pets %>      
<%= f.select :can_host_pets, options_for_select([ "Yes", "No"]) %>

 <%= f.label :has_pets %>         
<%= f.select :has_pets, options_for_select([ "Yes", "No"]) %>

 <%= f.label :current_online_learning %>    
 <%= f.select :current_online_learning, options_for_select([ "Edx", "Udacity", "Treehouse", "Coursera", "Khan Academy", "Lynda", "Standford Online", "MIT Free Coureware"]) %>

  <%= f.label :education %>         
  <%= f.text_field :education %>

  <%= f.label :languages %>     
  <%= f.text_field :languages %>
            
  <%= f.label :skills %>            
  <%= f.text_field :skills %>      

  <%= f.label :description %>       
  <%= f.text_field :description %>      
  
<div class="text-center"><%= f.submit class: "button radius" %></div>

<% end %>